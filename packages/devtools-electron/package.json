{
  "name": "@cnstra/devtools-electron",
  "version": "1.0.20",
  "private": true,
  "description": "CNStra DevTools desktop app (Electron) bundling DevTools server + panel UI",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "cross-env DEV=1 CNSTRA_DEVTOOLS_PORT=8080 electron .",
    "start": "electron .",
    "start:prod": "cross-env DEV=0 electron .",
    "gen:icons": "node ./scripts/gen-icons.js",
    "dist:mac": "electron-builder --config ./electron-builder.config.js --mac dmg zip",
    "dist:win": "electron-builder --config ./electron-builder.config.js --win nsis",
    "dist:linux": "electron-builder --config ./electron-builder.config.js --linux AppImage",
    "dist:mas": "electron-builder --config ./electron-builder.config.js --mac mas",
    "dist": "electron-builder --config ./electron-builder.config.js -mwl"
  },
  "build": "./electron-builder.config.js",
  "author": "Andrei Baikov <abaikov23@gmail.com>",
  "dependencies": {
    "@cnstra/devtools-server": "1.0.0",
    "@cnstra/devtools-server-repository-in-memory": "1.0.0",
    "sharp": "^0.33.4",
    "ws": "^8.18.0",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "electron": "^30.0.0",
    "electron-builder": "^24.13.3",
    "icon-gen": "^3.0.0"
  }
}


