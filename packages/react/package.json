{
  "name": "@cnstra/react",
  "version": "1.0.1",
  "description": "React bindings for CNS neural network-inspired event flow system",
  "keywords": [
    "react",
    "neural-network",
    "event-flow",
    "reactive",
    "context",
    "hooks"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "build": "tsup && tsc --emitDeclarationOnly",
    "build:types": "tsc --emitDeclarationOnly",
    "prepublishOnly": "npm run build"
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0",
    "@cnstra/core": ">=1.6.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  },
  "author": "Andrei Baikov <abaikov23@gmail.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  }
}
